<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/service128"
        android:id="@+id/imageView3"
        android:layout_below="@+id/imageView"
        android:layout_marginLeft="@dimen/dimen_50"
        android:layout_marginTop="@dimen/dimen_90"
        />

    <ImageView
        android:layout_width="@dimen/dimen_40"
        android:layout_height="@dimen/dimen_40"
        android:src="@mipmap/education128"
        android:id="@+id/imageView4"
        android:layout_below="@+id/imageView3"
        android:layout_alignLeft="@+id/imageView3"
        android:layout_marginTop="@dimen/dimen_10"

        />






        <ImageView
            android:id="@+id/center_box"
            android:layout_width="160dp"
            android:layout_height="180dp"
            android:background="@drawable/shape"
            android:layout_marginTop="90dp"
            android:layout_marginLeft="90dp"

            >
        </ImageView>
    <RelativeLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:id="@+id/center_box_frame"
        android:background="@color/dark_yellow"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="297dp"
        android:layout_marginRight="50dp"

        >
    </RelativeLayout>


    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/history128"
        android:layout_marginTop="@dimen/dimen_40"
        android:layout_marginLeft="@dimen/dimen_70"
        android:id="@+id/imageView" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/run"
        android:layout_marginTop="@dimen/dimen_28"
        android:layout_below="@+id/imageView"
        android:layout_marginLeft="210dp" />

    <ImageView
        android:layout_width="@dimen/dimen_40"
        android:layout_height="@dimen/dimen_40"
        android:id="@+id/imageView6"
        android:src="@mipmap/brain128"
        android:layout_alignTop="@+id/imageView2"
        android:layout_toLeftOf="@+id/imageView4"
        android:layout_toStartOf="@+id/imageView4" />

    <ImageView
        android:layout_width="@dimen/dimen_40"
        android:layout_height="@dimen/dimen_40"
        android:src="@mipmap/graph_circle"
        android:layout_alignBottom="@+id/imageView"
        android:layout_alignRight="@+id/imageView2"
        android:id="@+id/imageView5" />

    <ImageView
        android:layout_width="@dimen/dimen_40"
        android:layout_height="@dimen/dimen_40"
        android:id="@+id/imageView7"
        android:src="@mipmap/geography128"
        android:layout_below="@+id/imageView2"
        android:layout_alignLeft="@+id/imageView5"
        android:layout_alignStart="@+id/imageView5"
        android:layout_marginTop="@dimen/dimen_40"/>


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/text_layout"
        android:layout_below="@+id/center_box_frame"
        android:layout_marginTop="@dimen/dimen_30">


    </RelativeLayout>


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/title_text"
        android:text="@string/first_title"
        android:textSize="@dimen/text_26"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/center_box_frame"
        android:layout_marginTop="@dimen/dimen_50"
        android:textColor="@android:color/black"
        />




    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/desc_text"
        android:text="@string/first_desc"
        android:textSize="@dimen/text_16"
        android:gravity="center"
        android:layout_below="@+id/title_text"
        android:layout_marginTop="@dimen/activity_vertical_margin"
        android:layout_marginLeft="@dimen/activity_horizontal_margin"
        android:layout_marginRight="@dimen/activity_horizontal_margin"
        android:textColor="@android:color/black"/>

</RelativeLayout>